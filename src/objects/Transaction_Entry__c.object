    <validationRules>
        <fullName>DebitAmountValidation</fullName>
        <active>true</active>
        <description>Cannot debit an amount which is less than available balance</description>
        <errorConditionFormula>( ( Contact__r.Available_Balance__c  &lt;  Amount__c )  ||  (  Contact__r.Available_Balance__c  -  Amount__c  ) &lt; 3000.00 ) &amp;&amp;   ISPICKVAL( Type__c , &quot;Debit&quot;)</errorConditionFormula>
        <errorDisplayField>Amount__c</errorDisplayField>
        <errorMessage>Cannot debit , due to insufficient Balance.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TransactionDateCannotBeInPast</fullName>
        <active>true</active>
        <errorConditionFormula>Transaction_Date__c  &lt;  TODAY()</errorConditionFormula>
        <errorDisplayField>Transaction_Date__c</errorDisplayField>
        <errorMessage>Transaction Date Cannot Be In The Past</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TransactionValidationForActiveContacts</fullName>
        <active>false</active>
        <description>Transaction is possible only for active Contacts and not for inactive contacts.</description>
        <errorConditionFormula>ISPICKVAL(Contact__r.StatusPicklist__c, &quot;Inactive&quot;)</errorConditionFormula>
        <errorMessage>Cannot proceed the transaction as the contact is inactive</errorMessage>
    </validationRules>
